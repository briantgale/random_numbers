<h1>Migration Status</h1>

<table class="table">
  <thead>
    <tr>
      <th>Status</th>
      <th>ID</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @migration_data.each do |data| %>
      <tr>
        <% data.each do |item| %>
          <td><%= item %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
